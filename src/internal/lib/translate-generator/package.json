{
  "name": "@dr/translate-generator",
  "version": "0.5.0",
  "description": "A tool to scan source package files and generate translatable files",
  "main": "translate-generator.js",
  "loader": "loader",
  "dr": {
    "type": [
      "builder",
      "server"
    ],
    "builderPriority": "before @dr-core/browserify-builder",
    "config": {
      "server": {
        "scanMethodNames": [
          "drTranslate"
        ]
      }
    }
  },
  "dependencies": {
    "acorn": "^4.0.11",
    "acorn-jsx": "^3.0.1",
    "estraverse-fb": "^1.3.1",
    "estraverse": "^4.2.0",
    "cheerio": "^0.22.0",
    "mkdirp": "^0.5.1",
    "js-yaml": "^3.7.0",
    "through2": "^2.0.1",
    "browser-resolve": "^1.11.0",
    "pify": "^2.3.0"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "LJ",
  "license": "ISC"
}
