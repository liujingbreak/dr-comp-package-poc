{
	"appenders": [
		{
			"type": "console",
			"layout": {
				"type": "pattern",
				"pattern": "%[%c -%] %m"
			}
		}
	],
	"levels": {
		"[all]": "info",
		"test": "DEBUG",
		"@dr-core/browserify-builder": "info",
		"wfh": "INFO",
		"wfh.ManualChunkPlugin": "debug",
		"wfh.ManualChunkPlugin-fd": "info",
		"wfh.ManualChunkPlugin-d": "info",
		"require-injector": "info",
		"require-injector.node-inject": "info",
		"@dr/translate-generator": "info",
		"@dr-core/webpack2-builder": "info",
		"wfh.api-loader": "info",
		"@dr/template-builder": "info"
	},
	"comment-out-appender": {
		"type": "dateFile",
		"filename": "logs/server.log",
		"pattern": "-dd",
		"alwaysIncludePattern": false,
		"category": "[all]"
	}
}
