{
	"appenders": [
		{
			"type": "console"
		}, {
			"type": "dateFile",
		    "filename": "logs/server.log",
		    "pattern": "-dd",
		    "alwaysIncludePattern": false,
			"category": "[all]"
		}, {
			"type": "file",
			"filename": "logs/dependencyControl.log",
			"category": "@dr-core/browserify-builder",
			"maxLogSize": 512000,
			"numBackups": 1
		}
	],
	"levels": {
		"[all]": "DEBUG",
		"test": "DEBUG",
		"rwPackageJson": "INFO",
		"@dr-core/browserify-builder": "DEBUG",
		"packageInstallMgr": "INFO",
		"@dr/translate-generator": "DEBUG"
	}
}
